{
  "version": 2,
  "buildCommand": "cd client && npm install && npm run build",
  "outputDirectory": "client/dist",
  "framework": "vite",
  "public": true,
  "cleanUrls": true,
  "routes": [
    {
      "src": "/photos/(.*)",
      "dest": "/photos/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
} 